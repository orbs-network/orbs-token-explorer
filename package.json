{
  "name": "orbs-token-explorer",
  "version": "0.0.1",
  "description": "Orbs Token Explorer",
  "main": "index.js",
  "engines": {
    "node": "10.17.0"
  },
  "scripts": {
    "publish": "node release.js",
    "test": "npm run test-server",
    "type-check": "tsc -p ./src/client/tsconfig.json",
    "test-server": "jest --config ./src/server/__tests__/jest.config.js",
    "test-client": "jest --config ./src/client/__tests__/jest.config.js",
    "test-e2e": "npm run build && jest --config ./e2e/jest-e2e.config.js",
    "clean": "rimraf dist",
    "postinstall": "npm run build",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "start": "node index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "build-client": "cross-env NODE_ENV=production webpack -p",
    "dev-client": "webpack-dev-server -w",
    "build-server": "tsc -p ./src/server",
    "dev-server": "tsc-watch -p ./src/server --onSuccess \"node index.js\" --noClear",
    "dev": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev-server\" \"npm run dev-client\"",
    "build": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run build-server\" \"npm run build-client\""
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 120
  },
  "keywords": [
    "typescript",
    "react",
    "starter-kit",
    "webpack",
    "fullstack",
    "express",
    "express4",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/orbs-network/orbs-token-explorer.git"
  },
  "license": "MIT",
  "author": "Gil Amran",
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/find-up": "^4.0.0",
    "body-parser": "^1.19.0",
    "color": "^3.1.2",
    "copy-text-to-clipboard": "^2.1.1",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "find-up": "^4.1.0",
    "helmet": "^3.21.2",
    "material-color-hash": "^0.1.6",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "node-cache": "^5.0.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "random-seed": "^0.3.0",
    "react-hanger": "^2.1.3",
    "react-spinners": "^0.6.1",
    "recharts": "^1.8.5",
    "styled-components": "^4.4.1",
    "tslib": "^1.10.0",
    "winston": "^3.2.1",
    "winston-transport-rollbar": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@material-ui/core": "^4.6.0",
    "@material-ui/icons": "^4.5.1",
    "@testing-library/jest-dom": "^4.2.3",
    "@testing-library/react": "^9.3.2",
    "@types/color": "^3.0.0",
    "@types/enzyme": "^3.10.3",
    "@types/expect-puppeteer": "^3.3.3",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/helmet": "0.0.45",
    "@types/jest": "^24.0.22",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/material-ui": "^0.21.7",
    "@types/mysql": "^2.15.8",
    "@types/node": "^12.12.7",
    "@types/passport": "^1.0.2",
    "@types/passport-local": "^1.0.33",
    "@types/puppeteer": "^1.20.2",
    "@types/react": "^16.9.11",
